{
  "rewrites": [
    {
      "source": "/api/express/(.*)",
      "destination": "/api/express"
    },
    {
      "source": "/api/media/(.*)",
      "destination": "/api/media"
    },
    {
      "source": "/.well-known/(.*)",
      "destination": "/api/well-known/$1"
    }
  ],
  "regions": ["hnd1"]
}