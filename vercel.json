{
  "rewrites": [
    {
      "source": "/api/express/(.*)",
      "destination": "/api/express"
    },
    {
      "source": "/api/media/(.*)",
      "destination": "/api/media"
    },
    {
      "source": "/.well-known/:path*",
      "destination": "/api/.well-known/:path*"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/assetlinks",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
          }
        ]
    }
  ],
  "regions": ["hnd1"]
}